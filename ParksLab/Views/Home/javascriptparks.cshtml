@model ParksLab.Models.Parks

<h2>Parks</h2>
<input id="search" type="text" />
<hr />
<div id="target">
    @foreach (var park in Model.parks)
    {
        <h4>@park.Parkname</h4>
        <p><i>Borough: </i>@park.Borough</p>
        <p><i>Acres: </i>@park.Acres</p>
        <p><i>Description: </i>@Html.Raw(park.Description)</p>
    }

</div>

<script src="~/js/parks.js"></script>